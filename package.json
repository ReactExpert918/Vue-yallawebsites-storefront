{
  "name": "@decormatters/dm-theme",
  "version": "0.2.5",
  "license": "UNLICENSED",
  "description": "DecorMatters UI components",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "source": "src/index.js",
  "peerDependencies": {
    "react": "*",
    "react-dom": "*",
    "styled-components": "*",
    "react-circular-progressbar": "*",
    "@lottiefiles/react-lottie-player": "*",
    "react-share": "*"
  },
  "scripts": {
    "build": "rollup -c",
    "build-watch": "rollup -c -w",
    "start-playground": "cd playground && npm run start",
    "i-all": "npm i && cd playground && npm i",
    "dev": "npm-run-all --parallel build-watch start-playground"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "@rollup/plugin-babel": "^5.2.3",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "@rollup/plugin-replace": "latest",
    "@rollup/plugin-url": "latest",
    "@svgr/rollup": "latest",
    "npm-run-all": "latest",
    "rollup": "^2.38.5",
    "rollup-plugin-delete": "latest",
    "rollup-plugin-peer-deps-external": "latest",
    "styled-components": "*"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:decormatters/dm-theme.git",
    "directory": "@decormatters/dm-theme"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/decormatters"
  }
}
